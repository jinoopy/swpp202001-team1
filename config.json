{
    "llvm-bin-dir": "~/llvmscript/llvm-10.0-releaseassert/bin",
    "opt-pass": {
        "vectorize": {
            "src": "optim/VectorizeLoadStore.cpp",
            "lib": "libVLS"
        },
        "malloc2gv": {
            "src": "optim/MallocToGV.cpp",
            "lib": "libMTGV"
        },
        "gv2malloc": {
            "src": "optim/GVToMalloc.cpp",
            "lib": "libGVTM"
        },
        "heap2stack": {
            "src": "optim/HeapToStack.cpp",
            "lib": "libHTS"
        },
        "power-reduc": {
            "src": "optim/PowerReduction.cpp",
            "lib": "libPR"
        }
    },
    "preset-passes": {
        "global-optim": [
            "ipsccp",
            "globalopt",
            "globaldce",
            "gv2malloc",
            "mem2reg",
            "gvn"
        ],
        "function-optim": [
            "mergefunc",
            "tailcallelim",
            "inline"
        ],
        "memory-optim": [
            "vectorize",
            "heap2stack",
            "malloc2gv",
            "deadargelim",
            "mem2reg"
        ],
        "inst-optim": [
            "reassociate",
            "constprop",
            "instcombine",
            "sink",
            "sccp",
            "simplifycfg"
        ],
        "loop-optim": [
            "reassociate",
            "loop-simplify",
            "lcssa",
            "licm"
        ]
    },
    "run": {
        "opt": [],
        "backend": ""
    }
}