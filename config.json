{
    "llvm-bin-dir": "~/llvmscript/llvm-10.0-release/bin",
    "opt-pass": {
        "vectorize": {
            "src": "optim/VectorizeLoadStore.cpp",
            "lib": "libVLS"
        },
        "malloc2gv": {
            "src": "optim/MallocToGV.cpp",
            "lib": "libMTGV"
        },
        "gv2malloc": {
            "src": "optim/GVToMalloc.cpp",
            "lib": "libGVTM"
        },
        "heap2stack": {
            "src": "optim/HeapToStack.cpp",
            "lib": "libHTS"
        },
        "power-reduc": {
            "src": "optim/PowerReduction.cpp",
            "lib": "libPR"
        }
    },
    "preset-passes": {
        "global-optim": [
            "globaldce",
            "globalopt",
            "ipsccp",
            "gv2malloc",
            "mem2reg",
            "gvn"
        ],
        "function-optim": [
            "mergefunc",
            "tailcallelim",
            "inline"
        ],
        "memory-optim": [
            "heap2stack",
            "deadargelim",
            "malloc2gv",
            "mem2reg"
        ],
        "inst-optim": [
            "reassociate",
            "constprop",
            "power-reduc",
            "instcombine",
            "power-reduc",
            "sink",
            "sccp",
            "simplifycfg",
            "dce"
        ],
        "loop-optim-1": [
            "reassociate",
            "loop-simplify",
            "lcssa",
            "licm",
            "loop-rotate",
            "loop-unswitch"
        ],
        "loop-optim-2": [
            "reassociate",
            "loop-simplify",
            "indvars",
            "loop-unroll",
            "vectorize"
        ]
    },
    "run": {
        "opt": [
            "global-optim",
            "function-optim",
            "inst-optim",
            "memory-optim",
            "inst-optim",
            "loop-optim-1",
            "inst-optim",
            "loop-optim-2",
            "inst-optim"
        ],
        "backend": ""
    }
}