{
    "llvm-bin-dir": "~/llvmscript/llvm-10.0-release/bin",
    "opt-pass": {
        "vectorize": {
            "src": "optim/VectorizeLoadStore.cpp",
            "lib": "libVLS"
        },
        "malloc2gv": {
            "src": "optim/MallocToGV.cpp",
            "lib": "libMTGV"
        },
        "gv2malloc": {
            "src": "optim/GVToMalloc.cpp",
            "lib": "libGVTM"
        },
        "heap2stack": {
            "src": "optim/HeapToStack.cpp",
            "lib": "libHTS"
        },
        "power-reduc": {
            "src": "optim/PowerReduction.cpp",
            "lib": "libPR"
        }
    },
    "preset-passes": {
        "basic-optim": [
            "mem2reg",
            "ipsccp"
        ],
        "gv2malloc": [
            "gv2malloc"
        ],
        "inst-optim": [
            "reassociate",
            "power-reduc",
            "instcombine",
            "power-reduc",
            "sccp",
            "deadargelim",
            "dse",
            "adce"
        ],
        "loop-optim-1": [
            "loop-simplify",
            "loop-rotate",
            "lcssa",
            "licm",
            "loop-unswitch"
        ],
        "loop-vectorize": [
            "loop-simplify",
            "loop-rotate",
            "lcssa",
            "indvars",
            "loop-unroll",
            "gvn",
            "vectorize",
            "simplifycfg"
        ],
        "loop-optim-2": [
            "loop-unroll",
            "unroll-count=4",
            "unroll-allow-partial",
            "unroll-runtime"
        ],
        "heap2stack": [
            "heap2stack"
        ]
    },
    "run": {
        "opt": [],
        "backend": ""
    }
}