{
    "llvm-bin-dir": "~/llvmscript/llvm-10.0-releaseassert/bin",
    "opt-pass": {
        "vectorize": {
            "src": "VectorizeLoadStore.cpp",
            "lib": "libVLS.so"
        },
        "malloc2gv": {
            "src": "MallocToGV.cpp",
            "lib": "libMTGV.so"
        },
        "gv2malloc": {
            "src": "GVToMalloc.cpp",
            "lib": "libGVTM.so"
        }
    },
    "preset-passes": {
        "basic-optim": "-mem2reg, -icsccp",
        "inst-optim": "-reassociate -instcombine -scce -deadargelim -deadstoreelim -adce",
        "loop-vectorize": "-loop-simplify -loop-rotate -lcssa -indvars -loop-unroll -gvn -load lib/libVLS.so -simplifycfg",
        "loop-optim-1": "-loop-simplify -loop-rotate -lcssa -licm -loop-unswitch"
    }
}